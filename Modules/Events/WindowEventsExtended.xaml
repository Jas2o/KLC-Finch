<Window x:Class="KLC_Finch.WindowEventsExtended"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="WindowEventsExtended" Width="400" SizeToContent="Height" WindowStyle="ToolWindow" WindowStartupLocation="CenterOwner">
    <Grid Margin="5">
        <StackPanel>
            <ComboBox x:Name="cmbLogTypeExtended" IsTextSearchEnabled="True" IsEditable="True" />
            <Button x:Name="btnSelect" Margin="0,5,0,0" Content="Select" HorizontalAlignment="Center" Width="80" Click="btnSelect_Click" />
        </StackPanel>
    </Grid>
</Window>
